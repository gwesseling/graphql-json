{
    "name": "@gwesseling/graphql-codegen",
    "description": "graphql-codegen - a JSON based graphql codegen",
    "keywords": [
        "apollo",
        "graphql",
        "json",
        "codegen"
    ],
    "scripts": {
        "postinstall": "node ./install.js",
        "build": "tsup",
        "tsc": "tsc --pretty --noEmit",
        "test": "echo no tests specified"
    },
    "version": "0.0.3",
    "author": "gwesseling",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gwesseling/graphql-json.git",
        "directory": "packages/graphql-codegen"
    },
    "main": "build/index.cjs.js",
    "module": "build/index.esm.js",
    "typings": "build/index.d.ts",
    "bin": {
        "graphql-codegen": "bin/graphql-codegen"
    },
    "devDependencies": {
        "tsup": "8.0.2"
    },
    "optionalDependencies": {
        "@graphql-json/linux-arm": "0.0.1",
        "@graphql-json/linux-386": "0.0.1",
        "@graphql-json/linux-arm64": "0.0.1",
        "@graphql-json/linux-x64": "0.0.1",
        "@graphql-json/win32-arm64": "0.0.1",
        "@graphql-json/win32-386": "0.0.1",
        "@graphql-json/win32-x64": "0.0.1",
        "@graphql-json/darwin-arm64": "0.0.1",
        "@graphql-json/darwin-x64": "0.0.1"
    },
    "files": [
        "bin",
        "build",
        "README.md",
        "install.js"
    ]
}
